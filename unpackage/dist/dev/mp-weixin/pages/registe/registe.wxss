.wrap {
  position: relative;
}
.wrap .logo {
  width: 227.8rpx;
  height: 227.8rpx;
  border-radius: 50%;
  background-color: #528090;
  opacity: 0.3;
  position: absolute;
  top: 133.18rpx;
  left: 66.59rpx;
}
.wrap .infoList {
  position: absolute;
  top: 438.08rpx;
  left: 75.35rpx;
  width: 574.76rpx;
}
.wrap .infoList ._li {
  box-sizing: border-box;
  height: 134.93rpx;
  border-bottom: 3px solid #528090;
  font-size: 45.56rpx;
  color: #528090;
  padding: 59.58rpx 0 0 10.26rpx;
  letter-spacing: 3.26rpx;
}
.wrap .infoList ._li:last-child ._span.getCode {
  float: right;
  font-size: 38.55rpx;
  letter-spacing: 0;
}
.wrap .finish {
  width: 375rpx;
  height: 127.92rpx;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  left: 375rpx;
  top: 1500.73rpx;
  border-radius: 70.09rpx 0px 0px 0px;
  background-color: #528090;
}
.wrap .finish ._span {
  color: white;
  font-size: 45.56rpx;
  font-weight: normal;
  letter-spacing: 3.5rpx;
}

